{
        "name": "arch28",
        "type": "AE",
        "vectorizer": [
                        {
                                "type": "Linear",
                                "in": 21,
                                "out": 8,
                                "init": "xavier"
                        },
                        {
                                "type": "Tanh"
                        }
                ],
        "encoder": [
                        {
                                "type": "Conv1d",
                                "in": 8,
                                "out": 32,
                                "kernel": 5,
                                "padding": "same",
                                "init": "xavier"
                        },
                        {
                                "type": "Tanh"
                        },
                        {
                                "type": "Conv1d",
                                "in": 32,
                                "out": 32,
                                "kernel": 5,
                                "padding": "same",
                                "init": "xavier"
                        },
                        {
                                "type": "Tanh"
                        },
                        {
                                "type": "Conv1d",
                                "in": 32,
                                "out": 32,
                                "kernel": 5,
                                "init": "xavier"
                        },
                        {
                                "type": "Tanh"
                        },
                        {
                                "type": "Flatten"
                        },
                        {
                                "type": "Linear",
                                "in": 512,
                                "out": 10,
                                "init": "xavier"
                        },
                        {
                                "type": "Tanh"
                        }
                ],
        "decoder": [
                        {
                                "type": "Linear",
                                "in": 10,
                                "out": 512,
                                "init": "he"
                        },
                        {
                                "type": "ReLU"
                        },
                        {
                                "type": "Unflatten",
                                "in": 32,
                                "out": 16
                        },
                        {
                                "type": "ConvTranspose1d",
                                "in": 32,
                                "out": 32,
                                "kernel": 5,
                                "init": "he"
                        },
                        {
                                "type": "ReLU"
                        },
                        {
                                "type": "ConvTranspose1d",
                                "in": 32,
                                "out": 32,
                                "kernel": 5,
                                "padding": 2,
                                "init": "he"
                        },
                        {
                                "type": "ReLU"
                        },
                        {
                                "type": "ConvTranspose1d",
                                "in": 32,
                                "out": 8,
                                "kernel": 5,
                                "padding": 2,
                                "init": "he"
                        },
                        {
                                "type": "ReLU"
                        }
                ],
        "devectorizer": [
                        {
                                "type": "Linear",
                                "in": 8,
                                "out": 21,
                                "init": "xavier"
                        },
                        {
                                "type": "LogSoftmax"
                        }
                ]
}